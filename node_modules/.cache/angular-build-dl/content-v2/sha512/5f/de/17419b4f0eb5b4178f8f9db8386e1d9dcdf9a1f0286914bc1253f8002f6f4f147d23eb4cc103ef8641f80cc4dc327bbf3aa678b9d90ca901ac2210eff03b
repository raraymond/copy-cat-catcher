const _c0 = ["*"];
class BoardComponent {
    constructor(boardService, dialog) {
        this.boardService = boardService;
        this.dialog = dialog;
    }
    taskDrop(event) {
        Object(_angular_cdk_drag_drop__WEBPACK_IMPORTED_MODULE_1__["moveItemInArray"])(this.board.tasks, event.previousIndex, event.currentIndex);
        this.boardService.updateTasks(this.board.id, this.board.tasks);
    }
    openDialog(task, idx) {
        const newTask = { label: 'purple' };
        const dialogRef = this.dialog.open(_dialogs_task_dialog_component__WEBPACK_IMPORTED_MODULE_2__["TaskDialogComponent"], {
            width: '500px',
            data: task
                ? { task: Object.assign({}, task), isNew: false, boardId: this.board.id, idx }
                : { task: newTask, isNew: true }
        });
        dialogRef.afterClosed().subscribe(result => {
            if (result) {
                if (result.isNew) {
                    this.boardService.updateTasks(this.board.id, [
                        ...this.board.tasks,
                        result.task
                    ]);
                }
                else {
                    const update = this.board.tasks;
                    update.splice(result.idx, 1, result.task);
                    this.boardService.updateTasks(this.board.id, this.board.tasks);
                }
            }
        });
    }
    handleDelete() {
        this.boardService.deleteBoard(this.board.id);
    }
}
