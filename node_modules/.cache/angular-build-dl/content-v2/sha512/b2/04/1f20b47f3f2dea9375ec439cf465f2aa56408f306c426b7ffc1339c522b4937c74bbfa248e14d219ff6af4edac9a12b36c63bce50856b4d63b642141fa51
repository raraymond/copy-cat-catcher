    //   return this.db.collection('files').add( {...data,
    //     uid: user.uid,
    //     downloadURL: this.downloadURL,
    //     );
    // }
    //delete board
    deleteFile(fileId) {
        return this.db
            .collection('files')
            .doc(fileId)
            .delete();
    }
    getUserFiles() {
        return this.afAuth.authState.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["switchMap"])(user => {
            if (user) {
                return this.db
                    .collection('files', ref => ref.where('uid', '==', user.uid))
                    .valueChanges({ idField: 'id' });
            }
            else {
                return [];
            }
        }));
    }
}
